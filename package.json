    "@cjs": "rollup -c config/rollup.config.js --environment TARGET:@cjs",
    "@es": "rollup -c config/rollup.config.js --environment TARGET:@es"
  },
  "dependencies": {

  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-runtime": "^6.23.0",
