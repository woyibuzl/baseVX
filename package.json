  "main": "dist/base-vx.cjs.min.js",
  "module": "src/index.js",
  "author": {
    "name": "夜是美"
  },
  "keywords": [
    "baseVX",
    "base-vx",
    "vuex",
    "vx",
    "vue"
  ],
  "bugs": {
    "url": "https://github.com/yeshimei/baseVX/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yeshimei/baseVX.git"
  },
  "license": "MIT",
  "scripts": {
