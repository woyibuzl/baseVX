    "babel-preset-stage-3": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "rollup": "^0.63.4",
    "rollup-plugin-terser": "^1.0.1",
    "vue": "^2.5.18",
    "lodash": "^4.17.11"
  }
